{
  "name": "emiketic-starter-strapi",
  "private": true,
  "version": "1.2.0",
  "description": "Strapi starter with middleware / plugin additions and essential EMIKETIC JavaScript conventions",
  "repository": "git@github.com:emiketic/emiketic-starter-react-native.git",
  "author": "EMIKETIC Technologies <contact@emiketic.com>",
  "license": "MIT",
  "homepage": "https://www.emiketic.com",
  "keywords": [
    "strapi",
    "starter",
    "nodejs",
    "headless",
    "cms",
    "koa",
    "emiketic"
  ],
  "bugs": {
    "url": "https://github.com/emiketic/emiketic-starter-strapi/issues",
    "email": "contact@emiketic.com"
  },
  "scripts": {
    "postinstall": "husky install",
    "develop": "NODE_ENV=development ENV_PATH=.env.development strapi develop",
    "start": "NODE_ENV=production strapi start",
    "seed:bare": "yarn db:reset && NODE_ENV=development DB_SEED_MODE=fixtures-bare ENV_PATH=.env.development strapi develop",
    "seed:mocks": "yarn db:reset && NODE_ENV=development DB_SEED_MODE=fixtures-mocks ENV_PATH=.env.development strapi develop",
    "db:reset": "./scripts/db-reset.sh",
    "lint:check": "eslint --ext .js --ext .jsx ./",
    "lint:fix": "eslint --ext .js --ext .jsx ./ --fix",
    "format:check": "prettier --check \"**/*.{js,jsx,json}\"",
    "format:fix": "prettier --write \"**/*.{js,jsx,json}\"",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "release:dry": "standard-version --dry-run",
    "build": "strapi build",
    "strapi": "strapi"
  },
  "dependencies": {
    "@strapi/plugin-i18n": "4.3.2",
    "@strapi/plugin-users-permissions": "4.3.2",
    "@strapi/strapi": "4.3.2",
    "@strapi/utils": "4.3.2",
    "fs": "^0.0.1-security",
    "knex": "^2.0.0",
    "lodash": "^4.17.21",
    "pg": "^8.7.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.10.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "prettier": "^2.5.1",
    "standard-version": "^9.3.2"
  },
  "strapi": {},
  "engines": {
    "node": ">=14.19.1 <=16.x.x",
    "npm": ">=6.0.0"
  }
}
